{
  "name": "uuu-kit",
  "main": "./lib/main",
  "author": "brownerd <ben@brownerd.com>",
  "version": "0.0.0",
  "description": "Make the syntax and ui themes that UU can hack, export and import in seconds. This only works with UUU-SYNTAX and UUU-UI.",
  "keywords": [],
  "activationCommands": {
    "atom-workspace": [
      "uuu:syntaxExportStyles",
      "uuu:syntaxImportStyles",
      "uuu:uiExportStyles",
      "uuu:uiImportStyles",
      "uuu:exportThemeObject",
      "uuu:importThemeObject",
      "uuu:fontcheck",
      "uuu:fontsync",
      "uuu:reloadWindow",
      "uuu:runPreset1",
      "uuu:runPreset2",
      "uuu:runPreset3",
      "uuu:runPreset4",
      "uuu:runPreset5",
      "uuu:runPreset6",
      "uuu:runPreset7",
      "uuu:runPreset8",
      "uuu:runPreset9",
      "uuu:runPreset10",
      "uuu:runPresetOriginal1",
      "uuu:runPresetOriginal2",
      "uuu:saveTheme1",
      "uuu:saveTheme2",
      "uuu:saveTheme3",
      "uuu:saveTheme4",
      "uuu:saveTheme5",
      "uuu:useTheme1",
      "uuu:useTheme2",
      "uuu:useTheme3",
      "uuu:useTheme4",
      "uuu:useTheme5"
    ]
  },
  "repository": "https://github.com/brownerd/uuu-kit",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "electron-json-storage": "^2.0.0"
  }
}
